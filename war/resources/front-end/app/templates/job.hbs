<h2 id='title'>Printing document</h2>
<form class="form-horizontal">
    <div class="form-group">
        <label class="col-sm-2 control-label">Name</label>
        <div class="col-sm-10">
            <p class="form-control-static">{{document.name}}</p>
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-2 control-label">Size</label>
        <div class="col-sm-10">
            <p class="form-control-static">{{document.size}}</p>
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-2 control-label">Updated at</label>
        <div class="col-sm-10">
            <p class="form-control-static">{{document.updatedAt}}</p>
        </div>
    </div>
</form>
{{outlet}}