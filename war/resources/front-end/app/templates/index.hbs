<h2>Select document</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th></th>
            <th>Id</th>
            <th>Name</th>
            <th>#Pages</th>
            <th>Updated At</th>
        </tr>
    </thead>
    <tbody>
        {{#each doc in model}}
            <tr>
                <td>{{input type="checkbox"}}</td>
                <td>{{doc.id}}</td>
                <td>{{doc.name}}</td>
                <td>{{doc.size}}</td>
                <td>{{doc.updatedAt}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>

{{#link-to "job.printers" 1 }}<button type="button" class="btn btn-primary">Print</button>{{/link-to}}